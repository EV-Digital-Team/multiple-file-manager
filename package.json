{
  "name": "multiple-file-manager",
  "version": "1.1.1",
  "description": "windows application made with electron to rename multiple file with the same name just by single click",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . multiple-file-manager --overwrite --asar=true --platform=win32 --arch=ia32 --icon=multiple-file-manager.ico  --version-string.CompanyName=DhanyN --version-string.FileDescription=DhanyN --version-string.ProductName=\"Multiple File Manager\"",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "Multiple File Manager",
    "icon": "multiple-file-manager.ico"
  },
  "keywords": [
    "file",
    "file management",
    "editor"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-builder": "^20.0.5"
  },
  "dependencies": {
    "minikuna": "^0.4.0"
  }
}
